{
	"name": "nodejs-getting-started",
	"version": "1.0.0",
	"description": "End to end sample for running Node.js applications on Google Cloud Platform",
	"repository": "https://github.com/GoogleCloudPlatform/nodejs-getting-started",
	"private": true,
	"scripts": {
		"start": "node app.js",
		"e2e": "repo-tools test deploy",
		"test": "repo-tools test app",
		"cover": "nyc --cache npm test; nyc report --reporter=html"
	},
	"author": "Google Inc.",
	"contributors": [
		{
			"name": "Jon Wayne Parrott",
			"email": "jonwayne@google.com"
		},
		{
			"name": "Jonathan Simon",
			"email": "jbsimon@google.com"
		},
		{
			"name": "Jason Dobry",
			"email": "jdobry@google.com"
		},
		{
			"name": "Ace Nassri",
			"email": "anassri@google.com"
		}
	],
	"cloud-repo-tools": {
		"requiresKeyFile": true,
		"requiresProjectId": true,
		"test": {
			"app": {
				"args": [
					"app.js"
				],
				"msg": "Hello, world!"
			}
		}
	},
	"license": "Apache-2.0",
	"semistandard": {
		"globals": [
			"after",
			"afterEach",
			"before",
			"beforeEach",
			"describe",
			"it"
		]
	},
	"dependencies": {
		"body-parser": "^1.18.3",
		"bootbot": "^1.0.16",
		"express": "4.16.2",
		"forever": "^0.15.3",
		"localtunnel": "latest",
		"lowdb": "^1.0.0",
		"ngrok": "^3.0.1",
		"node-local-tunnel": "latest",
		"request": "^2.88.0",
		"shelljs": "latest",
		"sleep": "^5.2.3",
		"ws": "^6.0.0"
	},
	"devDependencies": {
		"@google-cloud/nodejs-repo-tools": "2.2.1",
		"ava": "0.25.0",
		"supertest": "3.0.0"
	},
	"engines": {
		"node": "8.x.x"
	}
}
